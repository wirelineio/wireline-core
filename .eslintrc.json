{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": "off",
    "consistent-return": "off",
    "import/prefer-default-export": "off",
    "max-len": [2, 120, 2, {
      "ignoreComments": true,
      "ignoreStrings": true,
      "ignoreTemplateLiterals": true,
      "ignoreUrls": true
      }
    ],
    "no-console": "off",
    "no-underscore-dangle": "off",
    "object-curly-newline": "off",
    "padded-blocks": "off",
    "react/prop-types": "off"
  }
}