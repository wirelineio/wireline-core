{
  "name": "@wirelineio/credentials",
  "version": "0.3.0",
  "description": "Wireline Credentials",
  "homepage": "https://github.com/wirelineio/wireline-core/tree/master/packages/credentials#readme",
  "bugs": {
    "url": "https://github.com/wirelineio/wireline-core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wirelineio/wireline-core.git"
  },
  "license": "GPL-3.0",
  "main": "index.js",
  "browser": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run build:protobuf && npm run build:babel",
    "build:babel": "babel ./src --out-dir ./dist --ignore \"**/*.test.js\" --source-maps=both --copy-files",
    "clean": "del-cli dist",
    "lint": "eslint ./src --ignore-path ../../.eslintignore",
    "lint:fix": "eslint --fix ./src --ignore-path ../../.eslintignore",
    "test": "DEBUG=${DEBUG:-'creds*'} jest --verbose src",
    "test:auth": "DEBUG=${DEBUG:-'creds*'} jest --verbose src/auth",
    "test:party": "DEBUG=${DEBUG:-'creds*'} jest --verbose src/party",
    "test:greet": "DEBUG=${DEBUG:-'creds*'} jest --verbose src/greet",
    "prepublishOnly": "npm run test && npm run build",
    "watch": "babel-watch ./src --out-dir ./dist --source-maps",
    "build:pb:auth": "pbjs -t json src/auth/auth.proto -o ./src/auth/auth.json",
    "build:pb:party": "pbjs -t json src/party/partyConstruction.proto -o ./src/party/partyConstruction.json",
    "build:pb:greet": "pbjs -t json src/greet/greet.proto -o ./src/greet/greet.json",
    "build:protobuf": "npm run build:pb:party && npm run build:pb:greet && npm run build:pb:auth"
  },
  "browserslist": [
    "> 5%"
  ],
  "dependencies": {
    "@wirelineio/codec-protobuf": "^0.3.0",
    "@wirelineio/framework": "^0.3.0",
    "@wirelineio/mixer": "^0.3.0",
    "@wirelineio/protocol": "^0.3.0",
    "@wirelineio/utils": "^0.3.0",
    "debug": "^4.1.1",
    "end-of-stream": "^1.4.1",
    "hypercore-crypto": "^1.0.0",
    "hypercore-protocol": "^6.11.1",
    "json-stable-stringify": "^1.0.1",
    "lokijs": "^1.5.8",
    "ngraph.graph": "^18.0.1",
    "pump": "^3.0.0",
    "queue-microtask": "^1.1.2",
    "source-map-support": "^0.5.12",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/preset-env": "^7.4.5",
    "babel-jest": "^24.8.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-inline-import": "^3.0.0",
    "del-cli": "^2.0.0",
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "ngraph.path": "^1.2.0",
    "wait-for-expect": "^3.0.0",
    "protobufjs": "https://github.com/wirelineio/protobuf.js#wireline-dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "0364698ca71eaf38d892bfc0c6f9b9c7d0f3cb00"
}
