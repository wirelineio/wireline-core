{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "10.10.0"
        },
        "modules": "commonjs"
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-inline-import", {
        "extensions": [
          ".proto",
          ".txt"
        ]
      }
    ],
    "add-module-exports",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-export-default-from",
    "@babel/plugin-transform-runtime"
  ]
}
