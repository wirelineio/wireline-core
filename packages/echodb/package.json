    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/preset-env": "^7.4.5",
    "@wirelineio/discovery-swarm-memory": "^0.2.17",
